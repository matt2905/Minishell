<START>		::=	[STRING SEP?]+

<STRING>	::= [OPE_R* CMD OPE_R* PARAM* OPE_R* OPE_P*]

<OPE_R>		::= [OPE_LR | OPE_RR | OPE_DRR | OPE_DLR]

<OPE_LR>	::=	"<" PARAM

<OPE_DLR>	::=	"<<" PARAM

<OPE_DRR>	::=	">>" PARAM

<OPE_RR>	::= ">" PARAM

<OPE_P>		::= STRING "|" STRING

<PARAM>		::= [LETTER | NUMBER | SPECIAL]+

<CMD>		::= [LETTER | NUMBER | SPECIAL]+

<SPECIAL>	::=	['}' | '{' | ['['...'`'] | '@' | '=' | ':' | ['''...'/'] |
['!'...'%']]

<SEP>		::= ';'

<LETTER>	::= ['a'..'z' | 'A'...'Z']
												//ALNUM
<NUMBER>	::= ['0'..'9']
